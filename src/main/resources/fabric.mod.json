{
  "schemaVersion": 1,
  "id": "lucid",
  "version": "${version}",
  "name": "Lucid",
  "description": "An Lucid for Meteor Client addons.",
  "authors": ["HUHNcode"],
  "contact": {
    "repo": "https://github.com/MeteorDevelopment/meteor-Lucid"
  },
  "icon": "assets/template/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": ["huhncode.lucid.lucidaddon.LucidAddon"]
  },
  "mixins": ["Lucid.mixins.json"],
  "custom": {
    "meteor-client:color": "225,25,25"
  },
  "depends": {
    "java": ">=21",
    "minecraft": ["${mc_version}"],
    "meteor-client": "*"
  }
}
